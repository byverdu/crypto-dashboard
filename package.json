{
  "name": "crypto-dashboard",
  "version": "1.0.0",
  "description": "webApp where you can see your crypto portfolio",
  "scripts": {
    "dev:api": "cd packages/api; yarn dev",
    "dev:client": "cd packages/client; yarn start",
    "dev:serverSendEvents": "cd packages/serverSendEvents; yarn dev",
    "dev": "concurrently --kill-others-on-fail \"yarn dev:api\" \"yarn dev:serverSendEvents\" \"yarn dev:client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byverdu/crypto-dashboard.git"
  },
  "author": "Albert Vallverdu <@byverdu>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/byverdu/crypto-dashboard/issues"
  },
  "homepage": "https://github.com/byverdu/crypto-dashboard#readme",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "lerna": "^3.10.5"
  }
}
