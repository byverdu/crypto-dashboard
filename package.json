{
  "name": "crypto-dashboard",
  "version": "1.0.0",
  "description": "webApp where you can see your crypto portfolio",
  "main": "index.js",
  "scripts": {
    "test:server": "jest --watch --testEnvironment=node",
    "dev:server": "babel-node ./app/server \"webpack-server\"",
    "sync": "browser-sync start --no-open --proxy 'localhost:9000' --files './app/client'",
    "dev:nodemon": "nodemon --exec babel-node ./app/server --watch ./app/server",
    "dev:react:server": "webpack --config ./webpack/webpack.dev.babel.js --watch",
    "dev:react": "webpack-dev-server --config ./webpack/webpack.dev.babel.js --progress --inline",
    "build": "webpack --config ./webpack/webpack.prod.babel.js",
    "dev:server:client": "parallelshell 'npm run dev:nodemon' 'npm run dev:react:server' 'npm run sync'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byverdu/crypto-dashboard.git"
  },
  "author": "Albert Vallverdu <@byverdu>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/byverdu/crypto-dashboard/issues"
  },
  "homepage": "https://github.com/byverdu/crypto-dashboard#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "del": "^3.0.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "parallelshell": "^3.0.2",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "webpack-watch-files-plugin": "^1.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "reactstrap": "^5.0.0-alpha.4"
  }
}
